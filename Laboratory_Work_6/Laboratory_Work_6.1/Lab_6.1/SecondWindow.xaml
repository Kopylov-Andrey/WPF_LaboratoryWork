<Window x:Class="Lab_6._1.SecondWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_6._1"
        mc:Ignorable="d"
        Title="SecondWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="75*"/>
            <ColumnDefinition Width="719*"/>
        </Grid.ColumnDefinitions>
        <Button Name="OkBtn" Click="OkBtn_Click" Content="Сжать" HorizontalAlignment="Left" Margin="35,30,0,0" VerticalAlignment="Top" Width="100" FontSize="14" Grid.ColumnSpan="2" Height="28"/>
        <Button Name="CancelBtn" Click="CancelBtn_Click" Content="Отмена" HorizontalAlignment="Right" Margin="0,30,530.6,0" VerticalAlignment="Top" Width="100" FontSize="14" Grid.Column="1" Height="28"/>
        <TextBox Name="Output" HorizontalAlignment="Left" Height="320" Margin="35,69,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="721" Grid.ColumnSpan="2"/>
    </Grid>
</Window>
